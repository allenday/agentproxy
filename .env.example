GEMINI_API_KEY=XXXX

# OpenTelemetry Configuration
AGENTPROXY_ENABLE_TELEMETRY=0

# Telemetry Data Privacy Controls
# Controls how task descriptions are exported to prevent sensitive data exposure
# Options: "none" (don't export), "hash" (hash only - default), "sanitized" (redact sensitive patterns), "full" (export all - DANGEROUS)
AGENTPROXY_TELEMETRY_EXPORT_TASK_DESCRIPTIONS=hash

# Maximum length for task descriptions if exported (default: 100)
AGENTPROXY_TELEMETRY_MAX_TASK_LENGTH=100
