{
  "title": "PA→Gemini",
  "uid": "pa-gemini",
  "tags": ["agentproxy", "gemini"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {"from": "now-1h", "to": "now"},
  "panels": [
    {
      "type": "text",
      "title": "",
      "gridPos": {"h": 2, "w": 24, "x": 0, "y": 0},
      "options": {"mode": "markdown", "content": "**PA→Gemini** - PA's calls to Gemini API: tokens, cost, latency, errors"}
    },
    {
      "type": "stat", "title": "API Requests",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 2},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "sum(increase(agentproxy_api_requests_total{api=\"gemini\"}[$__range])) or vector(0)", "refId": "A"}],
      "fieldConfig": {"defaults": {"unit": "none"}},
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}}
    },
    {
      "type": "stat", "title": "Tokens",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 2},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "sum(increase(agentproxy_tokens_consumed_total[$__range]))", "refId": "A"}],
      "fieldConfig": {"defaults": {"unit": "short"}},
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}}
    },
    {
      "type": "stat", "title": "Cost",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 2},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "sum(increase(agentproxy_api_cost_usd_total[$__range]))", "refId": "A"}],
      "fieldConfig": {"defaults": {"unit": "currencyUSD"}},
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}}
    },
    {
      "type": "stat", "title": "Errors",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 2},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "sum(increase(agentproxy_api_errors_total{api=\"gemini\"}[$__range])) or vector(0)", "refId": "A"}],
      "fieldConfig": {"defaults": {"unit": "none"}},
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}}
    },
    {
      "type": "timeseries", "title": "Token Usage Rate",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "sum(rate(agentproxy_tokens_prompt_total[5m])) * 60", "legendFormat": "Prompt", "refId": "A"},
        {"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "sum(rate(agentproxy_tokens_completion_total[5m])) * 60", "legendFormat": "Completion", "refId": "B"}
      ],
      "fieldConfig": {"defaults": {"unit": "short", "custom": {"axisLabel": "tokens/min"}}}
    },
    {
      "type": "timeseries", "title": "Cost Rate",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "sum(rate(agentproxy_api_cost_usd_total[5m])) * 3600", "legendFormat": "$/hr", "refId": "A"}],
      "fieldConfig": {"defaults": {"unit": "currencyUSD"}}
    },
    {
      "type": "timeseries", "title": "API Latency",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "histogram_quantile(0.50, sum(rate(agentproxy_gemini_api_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p50", "refId": "A"},
        {"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "histogram_quantile(0.95, sum(rate(agentproxy_gemini_api_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95", "refId": "B"}
      ],
      "fieldConfig": {"defaults": {"unit": "s"}}
    },
    {
      "type": "timeseries", "title": "Context Window Usage % (p50/p95)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "histogram_quantile(0.50, sum(rate(agentproxy_context_window_usage_percent_bucket[5m])) by (le, model))", "legendFormat": "p50 {{model}}", "refId": "A"},
        {"datasource": {"type": "prometheus", "uid": "prometheus"}, "expr": "histogram_quantile(0.95, sum(rate(agentproxy_context_window_usage_percent_bucket[5m])) by (le, model))", "legendFormat": "p95 {{model}}", "refId": "B"}
      ],
      "fieldConfig": {"defaults": {"unit": "percent", "max": 100}}
    }
  ]
}
